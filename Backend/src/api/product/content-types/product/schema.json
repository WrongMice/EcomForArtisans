{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 71,
      "minLength": 3
    },
    "slug": {
      "type": "uid",
      "required": true,
      "minLength": 3,
      "maxLength": 110
    },
    "description": {
      "type": "text"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "Painting",
        "Photography",
        "Sculpture",
        "Architecture",
        "Dye Art"
      ],
      "default": "Painting",
      "required": true
    },
    "price": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "AvailableQty": {
      "type": "integer",
      "default": 0,
      "required": true
    },
    "imgurl": {
      "type": "text"
    }
  }
}
